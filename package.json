{
  "name": "lifesaver",
  "displayName": "lifesaver",
  "description": "Display battery level in VS Code",
  "version": "0.0.1",
  "publisher": "StanMD",
  "engines": {
    "vscode": "^1.4.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Life Saver"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0",
    "battery-level": "^2.0.1"
  }
}
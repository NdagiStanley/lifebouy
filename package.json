{
  "name": "lifebuoy",
  "displayName": "lifebuoy",
  "description": "Lifebuoy saves you by showing you the time and battery level on the status bar",
  "version": "1.3.1",
  "publisher": "NdagiStanley",
  "repository": {
    "type": "git",
    "url": "https://github.com/NdagiStanley/lifebouy.git"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "theme": "light"
  },
  "activationEvents": [],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.saveLife",
        "title": "Lifebouy"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^4.0.1",
    "@types/node": "^8.0.47",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "battery-level": "^3.0.0",
    "date-and-time": "^0.5.0",
    "is-charging": "^1.1.0"
  }
}
